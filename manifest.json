{
	"manifest_version": 2,
	"name": "Reactive GitHub",
	"version": "0.1",
	"description": "Makes GitHub takes full browser width.",
	"content_scripts": [{
		"matches": [
			"https://github.com/*",
			"https://gist.github.com/*"
		],
		"css": ["inject.css"]
	}],
	"permissions": [
		"https://github.com/*",
		"https://gist.github.com/*"
	]
}
